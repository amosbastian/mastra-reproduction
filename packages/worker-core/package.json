{
  "version": "0.0.0",
  "name": "@acme/worker-core",
  "private": true,
  "type": "module",
  "exports": {
    "./tool": {
      "types": "./dist/tool/index.d.ts",
      "default": "./dist/tool/index.js"
    },
    "./workflow": {
      "types": "./dist/workflow/index.d.ts",
      "default": "./dist/workflow/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "bun run rm -rf dist *.tsbuildinfo",
    "dev": "tsc -w -p tsconfig.build.json",
    "start": "node dist/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@acme/cloudflare": "workspace:*",
    "@ai-sdk/google": "2.0.17",
    "@mastra/core": "0.20.0",
    "execa": "9.6.0",
    "zod": "4.1.11"
  },
  "devDependencies": {
    "@acme/eslint": "workspace:*",
    "@acme/typescript": "workspace:*",
    "@types/node": "24.7.0",
    "eslint": "9.37.0",
    "typescript": "5.9.3"
  }
}
