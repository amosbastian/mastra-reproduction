{
  "version": "0.0.0",
  "name": "@acme/worker-core",
  "private": true,
  "type": "module",
  "exports": {
    "./tool": {
      "types": "./dist/tool/index.d.ts",
      "default": "./dist/tool/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
