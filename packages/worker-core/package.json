    "typecheck": "tsc"
  },
  "dependencies": {
    "@mastra/core": "0.16.3",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@acme/eslint": "workspace:*",
    "@acme/typescript": "workspace:*",
    "@types/node": "24.3.1",
    "eslint": "9.35.0",
    "typescript": "5.9.2"
  }
}
